spring.profiles.active={{spring_profiles_active}}
server.port={{ service_port | default('8080') }}

spring.datasource.url=jdbc:mysql://{{ database_instance }}:{{ database_port }}/{{ database_schema_name }}?useSSL=false
spring.datasource.username={{ database_username }}
spring.datasource.password={{ database_password }}

logging.config=classpath:{{ log4j2_file_name }}

spring.rabbitmq.host={{ rabbitmq_host }}
spring.rabbitmq.port={{ rabbitmq_port }}
spring.rabbitmq.username={{ rabbitmq_username }}
spring.rabbitmq.password={{ rabbitmq_password }}

service.ribbon.enabled={{ ribbon_enabled }}

job.q.url=http://192.168.249.101:7007/ubs/mock/schedular
